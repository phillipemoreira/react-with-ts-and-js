{
  "setupFiles": [
    "<rootDir>/test-setup.js"
  ],
    "transform": {
    ".(js|jsx)": "<rootDir>/es6-preprocessor.js",
    ".(ts|tsx)": "<rootDir>/ts-preprocessor.js"
  },
  "mapCoverage": true,
  "coverageThreshold": {
      "global": {
          "statements": 90,
          "branches": 70,
          "functions": 90,
          "lines": 95
      }
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/build/"
  ],
  "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "collectCoverageFrom": [
    "app/**/*.{js,jsx,ts,tsx}",
    "!app/app.js",
    "!app/ts-types-samples/primitive-types.ts",
    "!app/ts-types-samples/objects.ts",
    "!app/ts-types-samples/union.ts",
    "!app/ts-types-samples/nulls.ts"
  ]
}