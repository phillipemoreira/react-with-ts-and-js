{
  "setupFiles": [
    "<rootDir>/test-setup.js"
  ],
    "transform": {
    ".(js|jsx)": "<rootDir>/es6-preprocessor.js",
    ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "mapCoverage": true,
  "coverageThreshold": {
      "global": {
          "statements": 100,
          "branches": 100,
          "functions": 100,
          "lines": 100
      }
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/build/"
  ],
  "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "collectCoverageFrom": [
    "app/**/*.{js,jsx,ts,tsx}",
    "!app/app.js"
  ]
}