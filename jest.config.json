{
  "setupFiles": [
    "<rootDir>/test-setup.js"
  ],
  "transform": {
    ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "mapCoverage": true,
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/build/"
  ],
  "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "collectCoverageFrom": [
    "app/**/*.{js,jsx,ts,tsx}"
  ]
}